auto-reload yes
reload-threads 100

cache-file "~/.config/newsboat/cache.db"

macro i set browser "feh %u"; open-in-browser ; set browser "lynx %u"

# unbind keys
unbind-key j
unbind-key k
unbind-key J
unbind-key K

# bind keys - vim style
bind-key j down
bind-key k up
bind-key l open
bind-key h quit
